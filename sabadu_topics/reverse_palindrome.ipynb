    "def reverse(text):\n",
    "    if len(text) <= 1:\n",
    "        return text\n",
    "    return reverse(text[1:]) + text[0]\n",
    "\n",
    "n = input(\"ENTER  STRING\\n\")\n",
    "if n==reverse(n):\n",
    "    print (\"It's a palindrome\")\n",
    "else:\n",
    "    print (\"It's not a palindrome\")"
